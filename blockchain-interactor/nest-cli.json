{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "plugins": [
    {
      "name": "@nestjs/swagger",
      "options": {
        "dtoFileNameSuffix": [
          ".dto.ts",
          ".params.ts"
        ],
        "controllerFileNameSuffix": ".controller.ts",
        "classValidatorShim": true,
        "introspectComments": true
      }
    }
  ]
}
